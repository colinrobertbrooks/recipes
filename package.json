{
  "name": "recipes",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier \"{src,__{tests,mocks}__}/**/*.+(ts|tsx|json|css)\"",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "test:watch": "react-scripts test",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "validate": "npm-run-all lint format:check test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-dom": "17.0.1",
    "react-highlight-words": "^0.17.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.9.0",
    "styled-components": "5.2.1",
    "use-query-params": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "@types/jest": "26.0.15",
    "@types/node": "12.0.0",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "axios-mock-adapter": "^1.19.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-colinrcummings": "^3.2.0",
    "gh-pages": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-scripts": "4.0.3",
    "typescript": "4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://colinrobertbrooks.github.io/recipes"
}
